NameVirtualHost *:80

<VirtualHost *:80>
  DocumentRoot "/var/www/subdomains/beta/"
  ServerName dev.beta.bikeplusapp.com

  <Directory "/var/www/subdomains/beta/">
    Options Indexes FollowSymLinks Includes ExecCGI
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>

  LogFormat "%h %l %u %t \"%r\" %>s %b" common
  ErrorLog /var/www/logs/beta.log
  CustomLog /var/www/logs/beta.log common
</VirtualHost>
