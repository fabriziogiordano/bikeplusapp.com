NameVirtualHost *:80

<VirtualHost *:80>
  DocumentRoot "/var/www/subdomains/dev/"
  ServerName dev.bikeplusapp.com

  <Directory "/var/www/subdomains/dev/">
    Options Indexes FollowSymLinks Includes ExecCGI
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>

  LogFormat "%h %l %u %t \"%r\" %>s %b" common
  ErrorLog /var/www/logs/dev.log
  CustomLog /var/www/logs/dev.log common
</VirtualHost>
