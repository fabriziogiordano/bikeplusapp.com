NameVirtualHost *:80

<VirtualHost *:80>
  DocumentRoot "/var/www/subdomains/assets/"
  ServerName dev.assets.bikeplusapp.com

  <Directory "/var/www/subdomains/assets/">
    Options Indexes FollowSymLinks Includes ExecCGI
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>

  LogFormat "%h %l %u %t \"%r\" %>s %b" common
  ErrorLog /var/www/logs/assets.log
  CustomLog /var/www/logs/assets.log common
</VirtualHost>
