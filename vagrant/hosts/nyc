NameVirtualHost *:80

<VirtualHost *:80>
  DocumentRoot "/var/www/subdomains/nyc/"
  ServerName nyc.loc.bikeplusapp.com

  <Directory "/var/www/subdomains/nyc/">
    Options Indexes FollowSymLinks Includes ExecCGI
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>

  LogFormat "%h %l %u %t \"%r\" %>s %b" common
  ErrorLog /var/www/logs/nyc.log
  CustomLog /var/www/logs/nyc.log common
</VirtualHost>
