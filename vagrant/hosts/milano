NameVirtualHost *:80

<VirtualHost *:80>
  DocumentRoot "/var/www/subdomains/milano/"
  ServerName milano.loc.bikeplusapp.com

  <Directory "/var/www/subdomains/milano/">
    Options Indexes FollowSymLinks Includes ExecCGI
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>

  LogFormat "%h %l %u %t \"%r\" %>s %b" common
  ErrorLog /var/www/logs/milano.log
  CustomLog /var/www/logs/milano.log common
</VirtualHost>
