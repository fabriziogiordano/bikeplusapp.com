<div data-dockid='{{=it.data.id}}' class="setbookmark"><strong></strong><span class="icon-bookmark {{? it.data.bookmarksAdded}}added{{?}}"></span></div>
<div class='close'><span></span></div>
<div class='dock'>
  <div class="dock_name">{{=it.data.sn}}</div>
  <span class="icon-bicycle"></span> {{=it.data.ab}}
  <span class="icon-unlock-stroke"></span> {{=it.data.ad}}
  {{? it.data.distance }}<span class='distance'> {{=it.lang.distance}} <strong>{{=it.data.distance}}</strong> {{=it.lang.unit}}</span>{{?}}
</div>

<button data-dockid='{{=it.data.id}}' value='{{=it.data.timer}}' class='timing {{? it.data.timer}}active{{?}}'>
  <div class="countdown"><span class="icon-stopwatch"></span></div>
  <strong>{{=it.data.timerlabel}}</strong>

  <!--<div class="sharetwitter disabled"><span class="icon-twitter"></span></div>-->
  <div class="sharefacebook {{=it.data.facebook}}"><span class="icon-facebook"></span></div>
</button>